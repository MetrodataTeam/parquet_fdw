-- import_parquet
create function list_parquet_files(args jsonb)
returns text[] as
$$
begin
    return array_agg(args->>'dir' || '/' || filename)
    from pg_ls_dir(args->>'dir') as files(filename)
    where filename ~~ '%.parquet';
end
$$
language plpgsql;
select import_parquet('example_import', 'public', 'parquet_srv', 'list_parquet_files', '{"dir": "/home/zilder/projects/parquet_fdw/data"}');
